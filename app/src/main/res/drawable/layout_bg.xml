<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#ffffff"
                android:startColor="#ffffff" />
            <stroke
                android:width=".2dp"
                android:color="@color/black" />
            <corners
                android:radius="3dp" />

        </shape>
    </item>
</selector>
